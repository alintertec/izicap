{
  "compilerOptions": {
    "target": "ES2015",
    "lib": [
      "ES2015",
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "types": [
      "cypress",
      "node"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": "src"
  },
  "include": [
    "src",
    "**/*.ts"
  ]
}